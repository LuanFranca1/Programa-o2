    }
    public static int  BuscaLinear(int n,int [] vet,int x){ // arrumar q ainda n estÃ¡ pegando;
        int i=1;
        while(i <= n && x>= vet[i]){
            if(vet[i] == x){
                return i;
            }
            i=i+1;
        }
        
        return -1;
    }
}
